          "sourceCode": "resource function get tomatoes(int quantity) returns string {\n        if quantity > 10 {\n            return \"large\";\n        } else if quantity > 5 {\n            if quantity > 3 {\n                return \"medium\";\n            } else if quantity > 1 {\n                return \"small\";\n            } else {\n                return \"tiny\";\n            }\n        } else {\n            return \"tiny\";\n        }\n    }"
